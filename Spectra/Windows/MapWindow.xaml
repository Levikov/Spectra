<Window x:Class="Spectra.MapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Spectra"
        mc:Ignorable="d"
        Title="Window_Map" Width="1024" Height="768" Loaded="windowMap_Loaded" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <WebBrowser x:Name="webMap"  Source="{x:Null}" Margin="0,0,0,0" SizeChanged="webMap_SizeChanged"/>
    </Grid>
</Window>
